{
    "type": "theme",
    "args": {
      "data": {
        "primaryColor": "#2c3ea4",
        "primaryColorBrightness": "dark"
      }
    },
    "child": {
      "type": "scaffold",
      "args": {
        "appBar": {
          "type": "app_bar",
          "args": {
            "title": {
              "type": "text",
              "args": {"text": "Card"}
            }
          }
        },
        "body": {
          "type": "center",
          "child": {
            "type": "container",
            "args": {"width": "240"},
            "child": {
              "type": "card",
              "args": {
                "color": "#E91E63",
                "elevation": 10,
                "shape": {"type": "rounded", "borderRadius": 15}
              },
              "child": {
                "type": "column",
                "args": {"mainAxisSize": "min"},
                "children": [
                  {
                    "type": "list_tile",
                    "args": {
                      "leading": {
                        "type": "icon",
                        "args": {
                          "icon": {
                            "codePoint": 57369,
                            "fontFamily": "MaterialIcons"
                          },
                          "size": 70
                        }
                      },
                      "title": {
                        "type": "text",
                        "args": {
                          "text": "Heart Shaker",
                          "style": {"color": "#fff"}
                        }
                      },
                      "subtitle": {
                        "type": "text",
                        "args": {
                          "text": "TWICE",
                          "style": {"color": "#fff"}
                        }
                      }
                    }
                  }
                ]
              }
            }
          }
        }
      }
    }
